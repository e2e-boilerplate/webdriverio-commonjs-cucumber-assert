    "@wdio/dot-reporter": "5.18.6",
    "@wdio/jasmine-framework": "5.18.6",
    "@wdio/local-runner": "5.18.6",
    "@wdio/sync": "5.18.6",
    "chromedriver": "79.0.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.1",
